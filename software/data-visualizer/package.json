{
  "name": "data-visualizer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:migrate": "dotenv -e .env.local -- npx prisma migrate dev --name",
    "db:deploy": "dotenv -e .env.local -- npx prisma migrate deploy",
    "db:studio": "dotenv -e .env.local -- npx prisma studio",
    "db:reset": "dotenv -e .env.local -- npx prisma migrate reset",
    "db:seed:local": "dotenv -e .env.local -- npx cross-env NODE_ENV=development tsx prisma/seed.ts",
    "vercel-build": "prisma generate && prisma migrate deploy && next build"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.964.0",
    "@aws-sdk/s3-request-presigner": "^3.964.0",
    "@prisma/client": "^6.6.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.0",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dlflib-js": "file:../dlflib-js",
    "dotenv": "^17.2.3",
    "jose": "^6.1.1",
    "leaflet": "^1.9.4",
    "leaflet.heat": "^0.2.0",
    "lucide-react": "^0.503.0",
    "next": "^16.0.7",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-leaflet": "^5.0.0",
    "tailwind-merge": "^3.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@serialport/parser-readline": "^13.0.0",
    "@tailwindcss/postcss": "^4",
    "@types/leaflet": "^1.9.17",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^10.1.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "prisma": "^6.6.0",
    "serialport": "^13.0.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.2.8",
    "typescript": "^5"
  }
}
