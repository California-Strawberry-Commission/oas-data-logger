generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma/client"
}

datasource db {
  provider = "postgresql"  
  url      = env("DATABASE_URL")
}



model Run {
  id         Int       @id @default(autoincrement())
  uuid       String    @unique
  epochTimeS Int
  tickBaseUs Int
  metadata   Json
  runData    RunData[]
}

model RunData {
  id         Int        @id @default(autoincrement())
  run        Run        @relation(fields: [runId], references: [id])
  runId      Int // relation scalar field
  streamType StreamType
  streamId   String
  tick       BigInt
  data       String
}

enum StreamType {
  POLLED
  EVENT
}
